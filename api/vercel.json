{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "clic-api",
  "regions": ["iad1"],
  "functions": {
    "api/index.ts": {
      "runtime": "edge",
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@neon_database_url"
  }
}
